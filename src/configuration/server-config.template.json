    "uploads": {
      "highSpeedUpload": "@externalLinks.uploads.highSpeedUpload@",
      "needHighSpeedUpload": "@externalLinks.uploads.needHighSpeedUpload@",
      "bulkUploadSamples": "@externalLinks.uploads.bulkUploadSamples@"
